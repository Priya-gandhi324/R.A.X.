        android:theme="@style/AppTheme">
        <activity android:name=".VerificationViaEmail">
            <intent-filter>
                <action android:name=".VerificationViaEmail" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".backto">
            <intent-filter>
                <action android:name=".backto" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
